{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Categories": {
      "type": "enumeration",
      "enum": [
        "T-shirts",
        "mugs",
        "hoodies",
        "stickers"
      ],
      "default": "T-shirts",
      "required": true
    },
    "size": {
      "type": "string",
      "required": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "green ",
        "yellow",
        "orange"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "availableQuantity": {
      "type": "integer",
      "default": 0,
      "required": true
    }
  }
}
